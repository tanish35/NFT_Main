    "react": "^18.2.0",
    "react-bootstrap": "^2.1.2",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.27.1",
    "react-router-dom": "^6.23.0",
    "stream-browserify": "3.0.0",
    "style-loader": "^3.2.1",
    "svg-url-loader": "7.1.1",
